{
  "name": "react-ratchet",
  "description": "Ratchet, rebuilt for React",
  "keywords": [
    "react",
    "ratchet",
    "react-component",
    "mobile",
    "ui"
  ],
  "author": "James Friend",
  "version": "0.3.0",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/jsdf/react-ratchet/master/LICENSE"
    }
  ],
  "scripts": {
    "test": "jest",
    "prepublish": "jsx --harmony src lib && jsx --harmony example example-build",
    "build": "6to5 src --out-dir lib && 6to5 example --out-dir example-build"
  },
  "files": [
    "index.js",
    "lib/",
    "vendor/",
    "bin/"
  ],
  "homepage": "https://github.com/jsdf/react-ratchet",
  "bugs": "https://github.com/jsdf/react-ratchet/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsdf/react-ratchet.git"
  },
  "dependencies": {
    "react": "0.13.x",
    "react-tools": "0.13.x"
  },
  "devDependencies": {
    "6to5": "^2.13.7",
    "babel-jest": "^5.2.0",
    "jest-cli": "^0.4.5",
    "react": "0.13.x"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ]
  }
}
